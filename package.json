{
  "private": true,
  "scripts": {
    "build:css": "lessc src/app.less dist/app.css --clean-css --no-ie-compat --source-map",
    "bundle:js": "rollup src/app.js -o dist/app.bundle --sourcemap=inline",
    "transpile:js": "babel dist/app.bundle --out-file dist/app.es5 --source-maps=inline --presets=es2015",
    "uglify:js": "uglifyjs dist/app.es5 -o dist/app.js --mangle --screw-ie8 --source-map-inline --source-map-root=../",
    "build:js": "npm run bundle:js && npm run transpile:js && npm run uglify:js",
    "build": "npm run build:css & npm run build:js",
    "watch": "watch \"npm run build\" src/"
  },
  "scripts": {},
  "dependencies": {
    "lodash": "4.16.6"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "less": "2.7.1",
    "less-plugin-clean-css": "1.5.1",
    "rollup": "0.36.3",
    "uglify-js": "2.7.4",
    "watch": "1.0.1"
  }
}
